LANGUAGE: feline

USING: feline ;
IN: feline

// this is a comment
( this is another comment )

: failed ( -- )
//     ?nl "failed" print
    "failed" throw
;

: check-t ( x -- )
    t eq? [ failed ] unless
;

: check-false ( x -- )
    f eq? [ failed ] unless
;

1 2 + 3 eq? check-t
1 2 + 4 eq? check-false

"test" "test" eq? check-false

V{ 1 2 3 } length 3 = check-t
V{ "this is a test" } length 1 = check-t

V{ 1 2 3 } [ dup + ] map V{ 2 4 6 } sequence= check-t

"this is a multiline
string" length 26 = check-t

clear

: multiline-string-test-1 ( -- )
    << last-word symbol-name >> ?nl write
    "this is
a multiline string"
    dup string? check-t
    length 26 = check-t
;

