feline!

true to use-tags?

require-system-file test-framework

only feline also definitions

test: test1 ( -- )
    1 fixnum? check
    2 fixnum? check
    3 fixnum? check
    1 2 + 3 = check
    1 2 + fixnum? check
;

test1

test: test2 ( -- )
    -1 fixnum? check
    -2 fixnum? check
    -3 fixnum? check
    -1 -2 + -3 = check
;

test2

test: test3 ( -- )
    0 fixnum? check
    0 0= check
    1 0= check-false
    -1 0= check-false
    1 -1 + 0= check
    4 -4 + 0= check
;

test3

test: test4 ( -- )
    1 2 u< check
    1 2 < check
;

test4

test: test5 ( -- )
    2 3 * 6 = check
;

test5

empty

?cr .( Reached end of fixnum-tests.feline )
