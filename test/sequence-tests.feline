require-system-file test-framework

LANGUAGE: feline

CONTEXT: feline forth ;
CURRENT: feline

\ mismatch
test: test1 ( -- )
    "test" "test" mismatch check-false
    "test" "testing" mismatch check-false
    "testing" "test" mismatch check-false
    "test" "" mismatch check-false
    "" "test" mismatch check-false
    "" "" mismatch check-false
    "test" "text" mismatch 2 = check-t

;

test1

\ head?
test: test2 ( -- )
    "test" "testing" head? check-false
    "testing" "test" head? check-t
    "test" "" head? check-t
    "" "test" head? check-false
;

test2

empty

?cr .( Reached end of sequence-tests )
